{
  "name": "brainfeed",
  "version": "1.0.0",
  "description": "A simple web app that shows application data from a json file",
  "main": "app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run serve --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juzoace/brainfeed.git"
  },
