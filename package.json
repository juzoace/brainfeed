  "bugs": {
    "url": "https://github.com/juzoace/brainfeed/issues"
  },
  "homepage": "https://github.com/juzoace/brainfeed#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nodemon": "^2.0.4"
  }
}
